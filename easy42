public class NthOddNumber {
    public static void main(String[] args) {
        int N = 7; // Sample input
        
        int count = 0;
        int i = 1;
        
        while (count < N) {
            if (i % 2 != 0) {
                count++;
                if (count == N) {
                    System.out.println("Nth Odd Number: " + i);
                    break;
                }
            }
            i++;
        }
    }
}
