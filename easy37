public class Factorial {
    public static void main(String[] args) {
        int N = 4; // Sample input
        
        if (N < 0) {
            System.out.println("Factorial is not defined for negative numbers.");
        } else {
            int factorial = 1;
            for (int i = 1; i <= N; i++) {
                factorial *= i;
            }
            System.out.println(N + " Factorial = " + factorial);
        }
    }
}
