public class PowerOfThree {
    public static void main(String[] args) {
        int n = 27; // Sample input
        
        if (n <= 0) {
            System.out.println("False");
            return;
        }
        
        while (n % 3 == 0) {
            n /= 3;
        }
        
        System.out.println(n == 1 ? "True" : "False");
    }
}
