public class ReverseNumber {
    public static void main(String[] args) {
        int number = 14567; // Sample input
        int reversedNumber = 0;
        
        while (number != 0) {
            int digit = number % 10;
            reversedNumber = reversedNumber * 10 + digit;
            number /= 10;
        }
        
        System.out.println("Reverse Number: " + reversedNumber);
    }
}
